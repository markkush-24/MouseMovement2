<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane fx:id = "pane" prefHeight="216.0" prefWidth="275.0" xmlns="http://javafx.com/javafx/8.0.202-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kushnirmark.javafx.ControlClass">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <ProgressBar fx:id="progressBar" prefHeight="34.0" prefWidth="190.0" progress="0.0" />
      <HBox alignment="CENTER" layoutX="-3.0" prefHeight="216.0" prefWidth="281.0">
         <children>
            <Button fx:id="buttonStart" mnemonicParsing="false" onMouseClicked="#clickedStart" text="Start" />
            <Button fx:id="buttonStop" mnemonicParsing="false" onMouseClicked="#clickedStop" text="Stop" />
         </children>
      </HBox>
   </children>
</Pane>
